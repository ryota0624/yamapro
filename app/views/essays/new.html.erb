 <head>
  <title>親こうfour</title>

  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <script src="//ajax.googleapis.com/ajax/libs/jquery/2.0.0/jquery.min.js"></script>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.3/css/materialize.min.css">
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.3/js/materialize.min.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/marked/0.3.5/marked.min.js"></script>
	<%= csrf_meta_tags %>
	   <%= stylesheet_link_tag 'ryo', 'data-turbolinks-track' => true %>
</head>
	
<body>
  <%= render "layouts/header" %>
    <%= javascript_tag do %>
    window.editor = {};
    window.editor.userId = <%= current_user.id %>;
    window.editor.redirect = "<%= raw root_path %>";
    window.editor.target  = "<%= img_path %>";
    window.editor.post = "<%= api_addessay_path %>";
    console.log(window.editor)
  	<% end %>
  <main>
    <div id="app"></div>
  </main>
  <%= render "layouts/footer" %>
  <%= javascript_include_tag "addjs/mdEditer.js" %>
</body>
</html>